def cp(n):
    for i in range(2, int(n ** (1/2)) + 1):    #zakres jest do sqrt(n) + 1, ponieważ nie ma sensu dalej sprawdzać (sqrt(n) * sqrt(n) = n i dalej po prostu sytuacja by była symetryczna)
        if(n % i == 0):
            return False
    return True

x = 0
def ciag(m):
    suma = 1
    j = 0
    for j in range (m + 1):     #m + 1 bo jest to przedział otwarty
        suma *= 2
    return (suma - 1)

def liczba(a):
    if(cp(a)):
        return int(a*(a+1)/2)
    return 0

while (liczba(ciag(x)) < 1000000):
    if(liczba(ciag(x)) != 0 ):
        print(liczba(ciag(x)))
    x += 1
